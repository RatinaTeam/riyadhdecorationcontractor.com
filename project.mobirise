          "_customHTML": "<section class=\"footer2\" group=\"Footers\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input inline type=\"range\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"2\">\n        <input inline type=\"range\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"2\">\n        <header>Show / Hide</header>\n        <input type=\"checkbox\" title=\"Logo\" name=\"showLogo\" checked>\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopyright\" checked>\n        <input type=\"checkbox\" title=\"Icons\" name=\"showIcons\">\n        <select title=\"Icons\" name=\"iconsCount\" condition=\"showIcons\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected>4</option>\n            <option value=\"5\">5</option>\n        </select>\n        <input type=\"checkbox\" title=\"Border Top\" name=\"showBorderTop\" checked>\n        <input type=\"color\" title=\"Border Top Color\" name=\"borderTopColor\" value=\"#d9dade\" condition=\"showBorderTop\">\n        <input type=\"color\" title=\"Circle Color\" name=\"circleColor\" value=\"#c1c2c5\" condition=\"showIcons\">\n        <input type=\"color\" title=\"Circle Bg Hover\" name=\"circleHoverColor\" value=\"#13287d\" condition=\"showIcons\">\n        <input type=\"color\" title=\"Icon Hover\" name=\"iconHoverColor\" value=\"#ffffff\" condition=\"showIcons\">\n\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" parallax>\n            <input type=\"color\" title=\"Background Color\" value=\"#f4efea\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay&&bg.type !== 'color'\">\n        <input type=\"range\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay&&bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container-fluid': fullWidth, 'container': !fullWidth }\">\n        <div class=\"media-container-row\">\n            <div class=\"col-lg-4 logo-footer mb-3 mt-3 align-right\">\n                <img class=\"img-logo-footer\" src=\"@PROJECT_PATH@/assets/images/best-jeddah-massage-logo.svg\" alt=\"Mobirise\" mbr-if=\"showLogo\">\n                \n            </div>\n            <div class=\"col-lg-4 copyright mb-3 mt-3\">\n                <p class=\"mbr-fonts-style mbr-regular text-copyright\" mbr-theme-style=\"display-4\" mbr-if=\"showCopyright\" data-app-selector=\".text-copyright\" style=\"font-size:12px\">© جميع الحقوق محفوظة لمقاول ديكورات الرياض<br><br></p>\n            </div>\n            <div class=\"social-media col-lg-4 col-sm-12\" mbr-if=\"showIcons\">\n                <ul>\n                    <li mbr-if=\"iconsCount > 0\">\n                        <a class=\"icon-transition\" href=\"#\">\n                            <span mbr-icon class=\"mbr-iconfont socicon-facebook socicon\">\n                            </span>\n                        </a>\n                    </li>\n                    <li mbr-if=\"iconsCount > 1\">\n                        <a class=\"icon-transition\" href=\"#\">\n                            <span mbr-icon class=\"mbr-iconfont socicon-twitter socicon\">\n                            </span>\n       